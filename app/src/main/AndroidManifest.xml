        <activity android:name=".adminApp.activity.AddUserActivity" />
        <activity android:name=".adminApp.activity.ViewAssignedBranchesActivity" />
        <activity android:name=".adminApp.activity.AddBranchActivity" />
        <activity android:name=".adminApp.activity.BranchCourseActivity" />
        <activity android:name=".adminApp.activity.RouteBusesActivity" />
